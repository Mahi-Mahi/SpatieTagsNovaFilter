{
    "name": "mahi/spatie-tags-nova-filter",
    "description": "A Laravel Nova filter for Spatie/Laravel-Tags.",

    "homepage": "https://github.com/Mahi-Mahi/SpatieTagsNovaFilter",
    "authors": [
        {
            "name": "Olivier Mourlevat",
            "email": "olivier.mourlevat@bronx.fr",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "nova",
        "tags",
        "filter",
        "spatie"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^3.0",
        "spatie/laravel-tags": "^2.3|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Mahi\\SpatieTagsNovaFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mahi\\SpatieTagsNovaFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
